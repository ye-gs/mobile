{
    "build": {
        "development": {
            "env": {
                "NODE_ENV": "development"
            },
            "developmentClient": true,
            "distribution": "internal",
            "channel": "development"
        },
        "development-no-dev-client": {
            "env": {
                "NODE_ENV": "development"
            },
            "developmentClient": false,
            "distribution": "internal",
            "channel": "development"
        },
        "test": {
            "developmentClient": false,
            "distribution": "internal",
            "channel": "development"
        },
        "preview": {
            "distribution": "internal",
            "channel": "preview"
        },
        "production": {
            "env": {
                "NODE_ENV": "production"
            },
            "android": {
                "image": "latest"
            },
            "ios": {
                "image": "latest"
            },
            "channel": "production"
        }
    }
}
